<!DOCTYPE html>
<html lang="en">
<head>
    <title>Object Information</title>
</head>
<body>


<div id="objectInfo">
  
    <p id="objectName">Object Name: "me"</p>

    <p id="objectDescription">Object Description: [Information about the object]</p>

    <button onclick="displayInfo()">Show Info</button>
</div>


<script>

    function displayInfo() {
        let name = "Sandro Oqropiridze";
        let description = "He does programming.";
        document.getElementById("objectName").innerText = "Object Name: " + name;
        document.getElementById("objectDescription").innerText = "Object Description: " + description;
    }
</script>

</body>
</html>
// first one


<!DOCTYPE html>
<html lang="en">
<head>
    <title>Car Objects</title>
</head>
<body>

<script>
    function Car(color, brand, doors) {
        this.color = color;
        this.brand = brand;
        this.doors = doors;
        this.startEngine = function () {
            console.log('Starting ' + this.brand + ' engine');
        };
    }

    const car1 = new Car('red', 'BMW', 'four');
    const car2 = new Car('white', 'Mercedes', 'four');

    console.log(car1);
    console.log(car2);

    car1.startEngine();
    car2.startEngine();
</script>

</body>
</html>
// second homework
